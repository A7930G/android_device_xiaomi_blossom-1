# System file
allow mtk_hal_light system_file:dir { getattr open read map ioctl search r_dir_perms };
allow mtk_hal_light sysfs:file { getattr open read write map ioctl r_file_perms rw_file_perms execute rx_file_perms };

allow mtk_hal_light sysfs_leds:lnk_file { getattr open read write map ioctl r_file_perms rw_file_perms };
